<application>
  <component name="XDebuggerSettings">
    <data-views />
    <general />
    <debuggers>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
          <stepping-filters>
            <stepping-filter url-pattern="express*" />
            <stepping-filter url-pattern="passport*" />
            <stepping-filter url-pattern="stripe*" />
            <stepping-filter url-pattern="*/node_modules/*" />
          </stepping-filters>
        </configuration>
      </debugger>
      <debugger id="kotlin_debugger">
        <configuration>
          <option name="DEBUG_IS_FILTER_FOR_STDLIB_ALREADY_ADDED" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>